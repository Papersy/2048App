            android:screenOrientation="portrait" />
        <activity
            android:name="com.juicyteam.a2048app.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
